{
  "name": "@anomalyhq/github-secrets-cli",
  "description": "Github secrets manager",
  "version": "1.0.1",
  "author": "Brendon Lees @brendon1555",
  "bin": {
    "ghs": "./bin/run"
  },
  "bugs": "https://github.com/anomaly/github-secrets-cli/issues",
  "dependencies": {
    "@oclif/command": "^1",
    "@oclif/config": "^1",
    "@oclif/errors": "^1",
    "@oclif/plugin-help": "^3",
    "@octokit/request": "^5.4.5",
    "fs-extra": "^9.0.1",
    "node-emoji": "^1.10.0",
    "tslib": "^1",
    "tweetsodium": "^0.0.5"
  },
  "devDependencies": {
    "@oclif/dev-cli": "^1",
    "@oclif/test": "^1",
    "@types/chai": "^4",
    "@types/fs-extra": "^9.0.1",
    "@types/mocha": "^5",
    "@types/node": "^14.0.20",
    "@types/node-emoji": "^1.8.1",
    "chai": "^4",
    "eslint": "^5.13",
    "eslint-config-oclif": "^3.1",
    "eslint-config-oclif-typescript": "^0.1",
    "globby": "^10",
    "mocha": "^5",
    "nyc": "^14",
    "ts-node": "^8",
    "typescript": "^3.3"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "files": [
    "/bin",
    "/lib",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json"
  ],
  "homepage": "https://github.com/anomaly/github-secrets-cli",
  "keywords": [
    "oclif",
    "github",
    "secrets",
    "cli",
    "command",
    "command line",
    "tools",
    "console"
  ],
  "license": "Apache-2.0",
  "main": "lib/index.js",
  "oclif": {
    "commands": "./lib/commands",
    "bin": "ghs",
    "plugins": [
      "@oclif/plugin-help"
    ]
  },
  "repository": "anomaly/github-secrets-cli",
  "scripts": {
    "postpack": "rm -f oclif.manifest.json",
    "posttest": "eslint . --ext .ts --config .eslintrc",
    "prepack": "rm -rf lib && tsc -b && oclif-dev manifest && oclif-dev readme",
    "test": "nyc --extension .ts mocha --forbid-only \"test/**/*.test.ts\"",
    "version": "oclif-dev readme && git add README.md",
    "prepare": "rm -rf lib && tsc -b"
  },
  "types": "lib/index.d.ts"
}